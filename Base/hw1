show databases;
use userpolyakov;
show tables;
select * from cars;
select * from cars where year>2000;
select * from cars where year<2015;
select * from cars where year between 2008 and 2010;
select * from cars where year !=2008;
select * from cars where year !=2009;
select * from cars where year !=2010;
select * from cars where price=year;
select * from cars where model like 'bmw' and year>2014;
select * from cars where model like 'audi' and year<2014;
select * from cars limit 5;
select * from cars limit 5 offset 212;
select avg(price) from cars where model like'Kia';
select avg(price) from cars;
select count(id) from cars;

